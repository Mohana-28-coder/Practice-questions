import java.util.Scanner;
public class hello{
 public static void main(String args[]){
   Scanner scan = new Scanner(System.in);
      int num=scan.nextInt();
      int sum=0;
      int temp=num;
     while(num>0){
     int digit=num%10;
     sum=sum+digit;
      num=num/10;
 
      }
      if(temp%sum==0){
       System.out.println("Harshad number");
      }
      else{
       System.out.println("Not a harshad number");
      }
 }
}
