import java.util.Scanner;
public class hello
{
	public static void main(String[] args) {
	   Scanner scan = new Scanner(System.in);
       int a=scan.nextInt();
       int n=scan.nextInt();
       int sum=0;
       int count=0;
       for(int i=a;i<=n;i++){
        if(i%2==0){
       
        sum=sum+i;
         
}
     if(i%2!=0){
      count=count+i;
     }
       }
        System.out.println(sum);
        System.out.println(count);
       
   }
}
   
