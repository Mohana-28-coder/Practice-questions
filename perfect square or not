import java.util.Scanner;
public class hello{
 public static void main(String args[]){
   Scanner scan = new Scanner(System.in);
      int n=scan.nextInt();
      int a=(int) Math.sqrt(n);
      if(a*a==n){
        System.out.println("Perfect square");
      }
      else{
        System.out.println("Not a perfect square");
      }
 }
}
